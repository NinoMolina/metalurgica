	<queryString>
		<![CDATA[SELECT
     ped."nropedido" AS ped_nropedido,
     c."razonsocial" AS c_razonsocial,
     f."nrofactura" AS f_nrofactura,
     fdp."nombre" AS fdp_nombre,
     cp."nrocomprobantepago" AS cp_nrocomprobantepago,
     cp."fechaemision" AS cp_fechaemision,
     cp."monto" AS cp_monto,
     f."montototal" AS f_montototal,
     f."fechavencimiento" AS f_fechavencimiento,
     f."fecharealcobro" AS f_fecharealcobro
FROM
     "factura" f,
     "formadepago" fdp,
     "pedido" ped,
     "cliente" c,
     "comprobantepago" cp
WHERE
     ped.factura = f.idfactura
     and f.formapago = fdp.idformapago
     and cp.factura = f.idfactura
     and ped.cliente= c.idcliente
     and cp.fechaemision >= $P{FECHA_DESDE}
     and cp.fechaemision < $P{FECHA_HASTA}

order by
 	c."razonsocial"]]>
	</queryString>
