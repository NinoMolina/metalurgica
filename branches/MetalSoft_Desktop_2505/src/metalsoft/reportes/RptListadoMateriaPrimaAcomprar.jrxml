	<queryString>
		<![CDATA[SELECT p.nropedido,
	mp.nombre,
	sum(dpp.cantmateriaprima) as Cantidad,
	dpp.preciomateriaprima as precio,
	p.nropedido as nroPedido,
	cli.razonsocial
FROM detalleproductopresupuesto dpp,
	detallepresupuesto dpre,
	detallepedido dped,
	materiaprima mp,
	pedido p,
	cliente cli
WHERE dpp.iddetallepresupuesto=dpre.iddetalle
	and dpre.iddetallepedido=dped.iddetalle
	and p.idpedido=$P{ID_PEDIDO}
	and dpp.idmateriaprima=mp.idmateriaprima and
	p.cliente=cli.idcliente
	and p.idpedido=dped.idpedido
GROUP BY mp.nombre,
	dpp.preciomateriaprima,
	p.nropedido,
	p.cliente,
	cli.razonsocial]]>
	</queryString>
