	<queryString>
		<![CDATA[SELECT
  cp.nrocomprobantepago,
  cp.fechaemision,
  fp.nombre AS formapago,
  cp.idcomprobantepago,
  cp.monto,
  f.nrofactura,
  cli.razonsocial,
  cli.cuit,
  ci.nombre AS iva,
  dom.calle,
  dom.numerocalle
FROM
  comprobantepago cp,
  formadepago fp,
  factura f,
  pedido p,
  cliente cli,
  domicilio dom,
  condicioniva ci
WHERE
  cp.factura = f.idfactura AND
  fp.idformapago = cp.formadepago AND
  p.factura = f.idfactura AND
  p.cliente=cli.idcliente AND
  cli.domicilio=dom.iddomicilio AND
  cli.condicioniva=ci.idcondicioniva AND
  cp.idcomprobantepago = $P{ID_COMPROBANTE}]]>
	</queryString>
