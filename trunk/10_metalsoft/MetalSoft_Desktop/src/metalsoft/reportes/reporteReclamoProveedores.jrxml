	<queryString>
		<![CDATA[SELECT
     rp."nroreclamo",
     p."razonsocial",
     rp."fechareclamo",
     c."nrocompra" AS c_nrocompra,
     rp."motivo" AS rp_motivo,
     er."nombre" AS er_nombre
FROM
     "reclamoproveedor" rp,
     "compra" c,
     "proveedor" p,
     "estadoreclamo" er
WHERE
     rp.compra = c.idcompra
     and c.proveedor = p.idproveedor
     and rp.idestadoreclamo = er.idestadoreclamo
     and rp.fechareclamo >= $P{FECHA_DESDE}
 AND rp.fechareclamo < $P{FECHA_HASTA}]]>
	</queryString>
