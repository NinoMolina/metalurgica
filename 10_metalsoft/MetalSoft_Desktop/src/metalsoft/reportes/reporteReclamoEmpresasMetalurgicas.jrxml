     rem."fechareclamo" AS rem_fechareclamo,
     er."nombre" AS er_nombre,
     rem."motivo" AS rem_motivo,
     em."razonsocial" AS em_razonsocial,
     tt."nrotrabajotercerizado" AS tt_nrotrabajotercerizado,
     rem."nroreclamo" AS rem_nroreclamo
FROM
     "reclamoempresametalurgica" rem,
     "trabajotercerizado" tt,
     "estadoreclamo" er,
     "empresametalurgica" em
WHERE
     rem.trabajotercerizado = tt.idtrabajo
     and tt.empresa = em.idempresametalurgica
     and rem.idestadoreclamo = er.idestadoreclamo
     and rem.fechareclamo >= $P{FECHA_DESDE}
 AND rem.fechareclamo < $P{FECHA_HASTA}]]>
