	<queryString>
		<![CDATA[SELECT c.nrocompra,
	mp.nombre,
	dc.cantidad,
    pxm.precio,
    p.razonsocial,
    p.cuit,
    (dom.calle || ' ' || dom.numerocalle || ', CP: ' || b.codpostal || ' - '
    || b.nombre || ' - ' || loc.nombre) AS dir
FROM compra c,
	proveedor p,
    detallecompra dc,
    materiaprima mp,
    proveedorxmateriaprima pxm,
    domicilio dom,
    localidad loc,
    barrio b
WHERE c.proveedor = p.idproveedor
	AND c.idcompra = dc.idcompra
    AND c.idcompra = $P{ID_PEDIDO}
    AND dc.materiaprima = mp.idmateriaprima
    AND pxm.idproveedor = p.idproveedor
    AND pxm.idmateriaprima = mp.idmateriaprima
    AND p.domicilio = dom.iddomicilio
    AND dom.barrio = b.idbarrio
    AND loc.idlocalidad = b.localidad]]>
	</queryString>
