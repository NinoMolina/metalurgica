	<queryString>
		<![CDATA[SELECT
     ped."nropedido" AS ped_nropedido,
     ped."fechapedidocotizacion" AS ped_fechapedidocotizacion,
     ped."fechaconfirmacionpedido" AS ped_fechaconfirmacionpedido,
     ep."nombre" AS ep_nombre,
     c."razonsocial" AS c_razonsocial,
     pre."montototal" AS pre_montototal
FROM
     factura f,
     formadepago fdp,
     pedido p,
     cliente c,
     comprobantepago cp,
WHERE
     ped.presupuesto = pre.idpresupuesto
     and ped.cliente = c.idcliente
     and ped.estado = ep.idestado
     and ped.fechapedidocotizacion >= $P{FECHA_DESDE}
     and ped.fechapedidocotizacion < $P{FECHA_HASTA}]]>
	</queryString>
