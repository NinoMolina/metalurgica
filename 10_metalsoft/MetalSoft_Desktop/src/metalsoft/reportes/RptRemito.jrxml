	<queryString>
		<![CDATA[SELECT
  ped.idpedido,
  ped.nropedido,
  cli.razonsocial,
  dom.calle,
  dom.numerocalle,
  cli.cuit,
  ci.nombre AS iva,
  rem.nroremito,
  rem.fechaemision,
  dr.cantidad AS cantidadremito,
  dr.descripcion AS descripcionremito,
  pro.nombre AS producto,
  pro.descripcion

FROM
  pedido ped,
  cliente cli,
  domicilio dom,
  condicioniva ci,
  remito rem,
  producto pro,
  detalleremito dr
WHERE
  ped.idpedido = $P{ID_PEDIDO} AND
  dr.idremito = rem.idremito AND
  rem.pedido = ped.idpedido AND
  pro.idproducto = dr.producto AND
  dom.iddomicilio = cli.domicilio AND
  ci.idcondicioniva = cli.condicioniva AND
  cli.idcliente = ped.cliente]]>
	</queryString>
