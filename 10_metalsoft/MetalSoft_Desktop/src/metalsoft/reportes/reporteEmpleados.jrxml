	<queryString>
		<![CDATA[SELECT
     emp."legajo" AS emp_legajo,
     emp."nombre" AS emp_nombre,
     emp."apellido" AS emp_apellido,
     cat."nombre" AS cat_nombre,
     car."nombre" AS car_nombre,
     emp."fechaingreso" AS emp_fechaingreso
FROM
     "empleado" emp,
     "categoria" cat,
     "cargo" car
WHERE
     emp.categoria = cat.idcategoria
     and emp.cargo = car.idcargo
order by
emp."legajo"]]>
