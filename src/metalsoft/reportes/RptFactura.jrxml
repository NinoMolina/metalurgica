	</parameter>
	<parameter name="MONTO" class="java.lang.Double"/>
	<queryString>
		<![CDATA[SELECT
  ped.idpedido,
  ped.nropedido,
  f.nrofactura,
  f.fechavencimiento,
  f.fechaemision,
  cli.razonsocial,
  dom.calle,
  dom.numerocalle,
  cli.cuit,
  ci.nombre AS iva,
  rem.nroremito,
  pro.nombre AS producto,
  pro.descripcion,
  dp.cantidad,
  dp.precio,
  df.montoparcial,
  df.cantidad AS cantidadfactura,
  fp.nombre,
  f.tipofactura,
  $P{MONTO} as monto

FROM
  pedido ped,
  factura f,
  cliente cli,
  domicilio dom,
  condicioniva ci,
  remito rem,
  producto pro,
  detallepedido dp,
  detallefactura df,
  formadepago fp
WHERE
  ped.factura = f.idfactura AND
  ped.idpedido = $P{ID_PEDIDO} AND
  df.idfactura = f.idfactura AND
  rem.pedido = ped.idpedido AND
  dp.idpedido = ped.idpedido AND
  pro.idproducto = dp.producto AND
  dom.iddomicilio = cli.domicilio AND
  ci.idcondicioniva = cli.condicioniva AND
  cli.idcliente = ped.cliente AND
  fp.idformapago=f.formapago AND
  df.iddetallepedido=dp.iddetalle
GROUP BY df.iddetalle,
 ped.idpedido,
  ped.nropedido,
  f.nrofactura,
  f.fechavencimiento,
  f.fechaemision,
  cli.razonsocial,
  dom.calle,
  dom.numerocalle,
  cli.cuit,
  ci.nombre,
  rem.nroremito,
  pro.nombre,
  pro.descripcion,
  dp.cantidad,
  dp.precio,
  df.montoparcial,
  df.cantidad,
  fp.nombre,
  f.tipofactura]]>
	</queryString>
