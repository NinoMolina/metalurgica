	<queryString>
		<![CDATA[select ('PEDI-'||p.nropedido) as nropedido, c.razonsocial,
('PROD-'||pro.nroproducto) as nroproducto,
pro.nombre AS nombre_producto,
('PIEZ-'||pir.nropieza) as nropieza, pi.nombre AS nombre_pieza

from detalleejecucionplanificacion dep,
detalleplanificacionproduccion dpp,
ejecucionplanificacionproduccion epp,
planificacionproduccion pp,
pedido p,
cliente c,
piezareal pir,
pieza pi,
producto pro

WHERE dep.id = dpp.iddetalleejecucionplanificacion and
dep.piezareal = pir.idpiezareal AND
dpp.idpieza = pi.idpieza and
dpp.idproducto = pro.idproducto AND
dep.idejecucionplanificacionproduccion = epp.idejecucion and
epp.idplanificacionproduccion = pp.idplanificacionproduccion and
pp.pedido = p.idpedido AND
p.cliente = c.idcliente AND
p.idpedido = $P{ID_PEDIDO}

GROUP BY nropieza, nropedido, nroproducto, nombre_producto, nombre_pieza, c.razonsocial]]>
