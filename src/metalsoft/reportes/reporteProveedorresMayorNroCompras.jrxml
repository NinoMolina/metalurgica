	<queryString>
		<![CDATA[select pro.idproveedor, pro.razonsocial, count(*)
from compra c, proveedor pro
where c.proveedor = pro.idproveedor
and c.fechacompra >= $P{Fecha_Desde}
and c.fechacompra < $P{Fecha_Hasta}
group by pro.idproveedor, pro.razonsocial]]>
